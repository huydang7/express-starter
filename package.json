{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build:esbuild": "node esbuild.js",
    "build:esbuild-dev": "npm run build:esbuild && node -r dotenv/config --inspect dist/index.js",
    "dev:esbuild": "nodemon --ext ts --exec \"npm run build:esbuild-dev\"",
    "build:swc": "swc src -d dist",
    "build:swc-dev": "npm run build:swc && node -r dotenv/config --inspect dist/index.js",
    "dev:swc": "nodemon --ext ts --exec \"npm run build:swc-dev\"",
    "dev": "rm -rf dist && webpack --config webpack.config.js --progress --watch",
    "start": "node -r dotenv/config dist/index.js",
    "format": "npx prettier --config .prettierrc --write ./src/**/*.ts ./src/*.ts",
    "lint": "npx eslint ./**/*.ts && echo 'Passed!'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^7.1.0",
    "esbuild": "^0.14.31",
    "express": "^4.17.3",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "http-status": "^1.5.0",
    "joi": "^17.6.0",
    "moment": "^2.29.1",
    "mongoose": "^6.2.9",
    "mongoose-delete": "^0.5.4",
    "mongoose-paginate-v2": "^1.6.3",
    "morgan": "^1.10.0",
    "nanoid": "^3.3.2",
    "nodemailer": "^6.7.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "regenerator-runtime": "^0.13.9",
    "validator": "^13.7.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.161",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose-delete": "^0.5.9",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
