{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build:esbuild": "node esbuild.js && npm run cp-resources",
    "build:esbuild-dev": "npm run build:esbuild && node -r dotenv/config --inspect dist/index.js",
    "dev:esbuild": "nodemon --ext ts --exec \"npm run build:esbuild-dev\"",
    "build:swc": "swc src -d dist && npm run cp-resources",
    "build:swc-dev": "npm run build:swc && node -r dotenv/config --inspect dist/index.js",
    "dev:swc": "nodemon --ext ts --exec \"npm run build:swc-dev\"",
    "build:tsc": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run cp-resources",
    "dev:tsc": "nodemon",
    "build": "npm run build:tsc",
    "dev": "nodemon",
    "start": "node -r dotenv/config dist/index.js",
    "format": "npx prettier --config .prettierrc --write ./src/**/*.ts ./src/*.ts",
    "lint": "npx eslint ./**/*.ts && echo 'Passed!'",
    "migration:dev": "dotenv -e ./.env sequelize-cli db:migrate",
    "migration:undo:dev": "dotenv -e ./.env sequelize-cli db:migrate:undo",
    "cp-resources": "cp .env dist/ && cp -R public dist/",
    "new": "hygen query new"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "dotenv-cli": "^6.0.0",
    "esbuild": "^0.15.8",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "http-status": "^1.5.3",
    "hygen": "^6.2.11",
    "joi": "^17.6.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.23.0",
    "sequelize-cli": "^6.4.1",
    "validator": "^13.7.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.2",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.7.18",
    "@types/nodemailer": "^6.4.6",
    "@types/passport": "^1.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/validator": "^13.7.6",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "4.8.3"
  },
  "type": "commonjs"
}
