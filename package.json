{
  "name": "express-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.ts",
  "scripts": {
    "build:swc": "swc src -d dist && npm run cp-resources",
    "build:swc-dev": "npm run build:swc && node -r dotenv/config --inspect dist/index.js",
    "dev:swc": "nodemon --ext ts --exec \"npm run build:swc-dev\"",
    "build:tsc": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json && npm run cp-resources",
    "dev:tsc": "nodemon",
    "build": "npm run build:tsc",
    "dev": "nodemon",
    "start": "node -r dotenv/config dist/index.js",
    "format": "prettier --config .prettierrc --write ./src/**/*.ts",
    "lint": "eslint --fix --ext .ts . && echo 'Passed!'",
    "new-migrate": "sequelize-cli migration:generate --name",
    "migrate": "dotenv -e ./.env sequelize-cli db:migrate",
    "migrate:undo": "dotenv -e ./.env sequelize-cli db:migrate:undo",
    "cp-resources": "cp .env dist/ && cp -R public dist/",
    "new": "hygen query new",
    "prepare": "husky",
    "postinstall": "husky",
    "commit": "cz"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "express": "^4.21.0",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "http-status": "^1.7.4",
    "husky": "^9.1.6",
    "hygen": "^6.2.11",
    "joi": "^17.13.3",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "path": "^0.12.7",
    "pg": "^8.13.0",
    "pg-hstore": "^2.3.4",
    "regenerator-runtime": "^0.14.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2",
    "validator": "^13.12.0",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@commitlint/prompt-cli": "^19.5.0",
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "1.7.4",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.5.5",
    "@types/nodemailer": "^6.4.16",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/validator": "^13.12.2",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.6",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.6.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
